function show(){
	document.getElementById('fox_hide').style.display = "none"  ;
	document.getElementById('fox_show').style.display = "block"  ;
	return false;
}
function hide(){
	document.getElementById('fox_hide').style.display = "block"  ;
	document.getElementById('fox_show').style.display = "none"  ;
	return false;
}
function showhelp(){
	document.getElementById('fox_hide').style.display = "none"  ;
	document.getElementById('fox_show').style.display = "block"  ;
	return true;
}
